; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GameJamProject"
run/main_scene="res://addons/dialogue_manager/example_balloon/small_example_balloon.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")

[autoload]

SceneLoader="*res://scripts/SceneLoader.cs"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/mode=3
window/size/initial_position_type=0
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="GameJamProject"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[internationalization]

locale/translations_pot_files=PackedStringArray("res://dialogues/start_dialogue.dialogue")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
